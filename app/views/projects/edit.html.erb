<% provide(:title, "Editing #{@project.name}") %>

<div class="container">
  <div class="center jumbotron">
    <h2>Editing Project "<%= @project.name %>"</h2>
    <%= form_for(@project) do |f| %>
      <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <%= f.submit 'Edit Project', id: 'edit-button', class: 'btn btn-large btn-primary' %>
    <% end %>
  </div>
</div>
